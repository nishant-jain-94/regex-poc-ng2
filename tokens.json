{
	"for\\s*\\(.*?\\)\\s*\\n*": {
		"type": "for",
		"push": "for",
		"supportsBlock": "true"
	},
	"if\\s*\\(.*\\)\\s*\\n*": {
		"type": "if",
		"push": "if",
		"supportsBlock": "true"
	},
	"\\{":{
		"type": "startblock",
		"push": "start of block"
	},
	"\\}": {
		"type": "endblock",
		"push": "end of block"
	}
}

